{
    "manifest_version": 3,
    "name": "Network Request Monitor",
    "version": "1.0",
    "permissions": [
      "webRequest",
      "tabs",
      "activeTab",
      "storage"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icons/icon16.jpg",
        "48": "icons/icon48.jpg",
        "128": "icons/icon128.jpg"
      }
    },
    "icons": {
      "16": "icons/icon16.jpg",
      "48": "icons/icon48.jpg",
      "128": "icons/icon128.jpg"
    },
    "host_permissions": [
      "<all_urls>"
    ]
  }
  