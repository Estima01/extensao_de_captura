{
  "manifest_version": 3,
  "name": "Network Request Monitor",
  "version": "1.0",
  "permissions": [
    "webRequest",
    "tabs",
    "activeTab",
    "storage"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/analise.png",
      "48": "icons/analise.png",
      "128": "icons/analise.png"
    }
  },
  "icons": {
    "16": "icons/analise.png",
    "48": "icons/analise.png",
    "128": "icons/analise.png"
  },
  "host_permissions": [
    "https://admin.anota.ai/*"
  ]
}
